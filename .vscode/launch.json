            "preLaunchTask": "npm: build",
            "program": "${workspaceFolder}/src/index.ts",
            "args": [
                "-H",
                "-v",
                "--pretty",
                "3",
                "set-timestamps"
            ],
            "outFiles": [
                "${workspaceFolder}/lib/**/*.js",
                "!**/node_modules/**",
                "!codes.json"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Update comments dock to waiting",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "preLaunchTask": "npm: build",
            "program": "${workspaceFolder}/src/index.ts",
            "args": [
                "-v",
                "--pretty",
                "3",
                "update-dock-redirect",
                "--waiting-redirect",
                "--path-file",
                "D:\\videos\\obs-content\\assets\\yt.html"
            ],
            "outFiles": [
                "${workspaceFolder}/lib/**/*.js",
                "!**/node_modules/**",
                "!codes.json"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Get stream info",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "preLaunchTask": "npm: build",
            "program": "${workspaceFolder}/src/index.ts",
            "args": [
                "--pretty",
                "3",
                "info"
            ],
            "outFiles": [
                "${workspaceFolder}/lib/**/*.js",
                "!**/node_modules/**",
                "!codes.json"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Help",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "preLaunchTask": "npm: build",
            "program": "${workspaceFolder}/src/index.ts",
            "env": {
                "cmdv": "v2"
            },
            "args": [
                "--help"
            ],
            "outFiles": [
                "${workspaceFolder}/lib/**/*.js",
                "!**/node_modules/**",
                "!codes.json"
            ]
        }
    ]
}