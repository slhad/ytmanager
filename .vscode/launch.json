    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Set-current-stream",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "preLaunchTask": "npm: build",
            "program": "${workspaceFolder}/src/index.ts",
            "args": [
                "-H",
                "--pretty",
                "3",
                "set-current-stream",
                "--playlist",
                "Destiny 2",
                "--playlist",
                "Streams",
                "--language",
                "fr",
                "--language-sub",
                "fr-FR",
                "--tag",
                "dailies",
                "--tag",
                "dust",
                "--tag",
                "farm",
                "--tag",
                "mature",
                "--category",
                "Gaming"
            ],
            "outFiles": [
                "${workspaceFolder}/lib/**/*.js",
                "!**/node_modules/**",
                "!codes.json"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "set-live-stream",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "preLaunchTask": "npm: build",
            "program": "${workspaceFolder}/src/index.ts",
            "args": [
                "-H",
                "--pretty",
                "3",
                "set-live-stream",=
                "--title",
                "test title",
                "--description",
                "test description"
            ],
            "outFiles": [
                "${workspaceFolder}/lib/**/*.js",
                "!**/node_modules/**",
                "!codes.json"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Update settings lib",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "preLaunchTask": "npm: build",
            "program": "${workspaceFolder}/src/index.ts",
            "args": [
                "-H",
                "stream-settings",
                "--vertical-link-offset",
                "1"
            ],
            "outFiles": [
                "${workspaceFolder}/lib/**/*.js",
                "!**/node_modules/**",
                "!codes.json"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Upload verticals",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "preLaunchTask": "npm: build",
            "program": "${workspaceFolder}/src/index.ts",
            "args": [
                "-H",
                "verticals-upload"
            ],
            "outFiles": [
                "${workspaceFolder}/lib/**/*.js",
                "!**/node_modules/**",
                "!codes.json"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Vertical-saved",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "preLaunchTask": "npm: build",
            "program": "${workspaceFolder}/src/index.ts",
            "args": [
                "-H",
                "--pretty",
                "3",
                "vertical-saved"
            ],
            "outFiles": [
                "${workspaceFolder}/lib/**/*.js",
                "!**/node_modules/**",
                "!codes.json"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Test mature tag",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "preLaunchTask": "npm: build",
            "program": "${workspaceFolder}/src/index.ts",
            "args": [
                "--pretty",
                "3",
                "set-current-stream",
                "--tag",
                "mature"
            ],
            "outFiles": [
                "${workspaceFolder}/lib/**/*.js",
                "!**/node_modules/**",
                "!codes.json"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Set thumbnail",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "preLaunchTask": "npm: build",
            "program": "${workspaceFolder}/src/index.ts",
            "args": [
                "-v",
                "--pretty",
                "3",
                "set-current-thumbnail",
                "--auto-recompress-on-limit",
                "--path-dir",
                "D:\\videos\\stream"
            ],
            "outFiles": [
                "${workspaceFolder}/lib/**/*.js",
                "!**/node_modules/**",
                "!codes.json"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Update comments dock",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "preLaunchTask": "npm: build",
            "program": "${workspaceFolder}/src/index.ts",
            "args": [
                "-H",
                "-v",
                "--pretty",
                "3",
                "update-dock-redirect",
                "--path-file",
                "D:\\videos\\obs-content\\assets\\yt.html"
            ],
            "outFiles": [
                "${workspaceFolder}/lib/**/*.js",
                "!**/node_modules/**",
                "!codes.json"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Update timestamps",
            "skipFiles": [
